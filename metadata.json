{
  "name": "abstractit-puppet",
  "version": "1.8.2",
  "author": "Abstract IT",
  "license": "Apache-2.0",
  "summary": "Manage Puppet agent, master, modules and hiera with Puppet",
  "source": "https://github.com/abstractitptyltd/abstractit-puppet",
  "project_page": "https://github.com/abstractitptyltd/abstractit-puppet",
  "issues_url": "https://github.com/abstractitptyltd/abstractit-puppet/issues",
  "tags": ["puppet","puppetdb","r10k","hiera"],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=3.6.2 <5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/apache",
      "version_requirement": ">=1.4.0 <2.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">=1.2.0 <2.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.6.0 <5.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">=1.8.0 <2.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">=1.2.0 <2.0.0"
    },
    {
      "name": "puppetlabs/postgresql",
      "version_requirement": ">=4.3.0 <5.0.0"
    },
    {
      "name": "puppetlabs/puppetdb",
      "version_requirement": ">=4.2.1 <5.0.0"
    },
    {
      "name": "zack/r10k",
      "version_requirement": ">=2.7.0 <3.0.0"
    }
  ]
}